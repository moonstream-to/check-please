{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://data.moonstream.to/schemas/check-please.json",
  "title": "Check, Please",
  "description": "JSON schema for *Check, Please* checklists",
  "type": "object",
  "properties": {
    "requester": {
        "type": "string",
        "description": "Address or other string identifying the person who is requesting completion of this checklist"
    },
    "executor": {
        "type": "string",
        "description": "Ethereum address of the person who should execute this checklist"
    },
    "description": {
        "type": "string",
        "description": "Description of the checklist"
    }
  },
  "$defs": {
    "steps": {}
  },
  "$comment": "JSON Schema spec: https://json-schema.org/draft/2020-12/json-schema-core"
}
